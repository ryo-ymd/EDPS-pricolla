<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @picture.name %>
</p>

<p>
  <strong>User:</strong>
  <%= @picture.key %>
</p>

<p>
  <strong>Type:</strong>
  <%= @picture.temp_type %>
</p>

<p>
